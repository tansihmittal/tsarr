{
  "name": "tsarr.in - Free Online Screenshot Editor",
  "short_name": "tsarr.in",
  "description": "Free online screenshot editor with frames, backgrounds, annotations. Create beautiful code screenshots, text behind image effects, video captions, and more.",
  "start_url": "/app",
  "id": "/app",
  "scope": "/",
  "lang": "en-US",
  "dir": "ltr",
  "display": "standalone",
  "display_override": ["tabbed", "window-controls-overlay", "standalone", "minimal-ui"],
  "orientation": "any",
  "background_color": "#ffffff",
  "theme_color": "#4f46e5",
  "categories": ["utilities", "productivity", "photo"],
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7",
  "prefer_related_applications": false,
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://tsarr.in/site.webmanifest",
      "id": "/app"
    }
  ],
  "icons": [
    {
      "src": "/favicon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/favicon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/favicon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "screenshots": [
    {
      "src": "/images/editor-preview.png",
      "sizes": "1920x1080",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Screenshot Editor on Desktop"
    },
    {
      "src": "/images/tbi-preview.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Text Behind Image on Mobile"
    }
  ],
  "shortcuts": [
    {
      "name": "Screenshot Editor",
      "short_name": "Editor",
      "url": "/editor",
      "description": "Edit and beautify screenshots",
      "icons": [{ "src": "/favicon-192x192.png", "sizes": "192x192" }]
    },
    {
      "name": "Code Screenshots",
      "short_name": "Code",
      "url": "/code",
      "description": "Create beautiful code images",
      "icons": [{ "src": "/favicon-192x192.png", "sizes": "192x192" }]
    },
    {
      "name": "All Tools",
      "short_name": "Tools",
      "url": "/tools",
      "description": "Browse all available tools",
      "icons": [{ "src": "/favicon-192x192.png", "sizes": "192x192" }]
    }
  ],
  "file_handlers": [
    {
      "action": "/editor",
      "accept": {
        "image/png": [".png"],
        "image/jpeg": [".jpg", ".jpeg"],
        "image/webp": [".webp"],
        "image/gif": [".gif"],
        "image/bmp": [".bmp"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+tsarr",
      "url": "/editor?url=%s"
    }
  ],
  "share_target": {
    "action": "/editor",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "image",
          "accept": ["image/*"]
        }
      ]
    }
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "handle_links": "preferred",
  "scope_extensions": [
    { "origin": "https://tsarr.in" },
    { "origin": "https://*.tsarr.in" },
    { "origin": "https://www.tsarr.in" }
  ],
  "tab_strip": {
    "home_tab": "auto",
    "new_tab_button": "auto"
  },
  "widgets": [
    {
      "name": "Quick Tools",
      "description": "Quick access to tsarr.in tools",
      "tag": "tsarr-widget",
      "ms_ac_template": "widget/quick-tools.json",
      "screenshots": [
        {
          "src": "/images/editor-preview.png",
          "sizes": "1920x1080",
          "label": "Quick Tools Widget"
        }
      ],
      "icons": [
        { "src": "/favicon-192x192.png", "sizes": "192x192" }
      ]
    }
  ],
  "note_taking": {
    "new_note_url": "/editor"
  }
}
